{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-google-gemini",
  "title": "Google Gemini",
  "description": "Use the Google Gemini API in Raycast.",
  "icon": "command-icon.png",
  "author": "blindfs",
  "contributors": [
    "EvanZhouDev"
  ],
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "ask-ai",
      "title": "Ask AI",
      "subtitle": "Google Gemini",
      "description": "Ask Google Gemini",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "summarize",
      "title": "Summarize",
      "subtitle": "Google Gemini",
      "description": "Summarize selected text",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "explain",
      "title": "Explain",
      "subtitle": "Google Gemini",
      "description": "Explain selected text",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "comment",
      "title": "Add Comments to Code",
      "subtitle": "Google Gemini",
      "description": "Add comments to code",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "friendly",
      "title": "Change Tone to Friendly",
      "subtitle": "Google Gemini",
      "description": "Change tone of selected text to friendly",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "professional",
      "title": "Change Tone to Professional",
      "subtitle": "Google Gemini",
      "description": "Change tone of selected text to professional",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "synonym",
      "title": "Find Synonym",
      "subtitle": "Google Gemini",
      "description": "Find synonym of selected word",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "image-qa",
      "title": "Image QA",
      "subtitle": "Google Gemini",
      "description": "Answer question in text, given the image, from clipboard by default",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": true
        },
        {
          "name": "docLink",
          "placeholder": "Image Link",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "doc-qa",
      "title": "Document QA",
      "subtitle": "Google Gemini",
      "description": "Answer question given extra context in url, url from clipboard by default",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": true
        },
        {
          "name": "docLink",
          "placeholder": "Doc Link",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "search-qa",
      "title": "Search QA",
      "subtitle": "Google Gemini",
      "description": "Answer question based on Google search results",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": true
        },
        {
          "name": "searchQuery",
          "placeholder": "Query to Google",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "iterative-rag",
      "title": "Iterative RAG",
      "subtitle": "Google Gemini",
      "description": "Answer questions based on multiple rounds of Google search and document reading",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "history",
      "title": "Search Chat History",
      "subtitle": "Google Gemini",
      "description": "Search chat history",
      "mode": "view"
    },
    {
      "name": "grammar",
      "title": "Fix Spelling and Grammar",
      "subtitle": "Google Gemini",
      "description": "Fix spelling and grammar of selected text",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "longer",
      "title": "Make Longer",
      "subtitle": "Google Gemini",
      "description": "Make selected text longer",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "shorter",
      "title": "Make Shorter",
      "subtitle": "Google Gemini",
      "description": "Make selected text shorter",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "preferences": [
    {
      "description": "Gemini API Key",
      "name": "apiKey",
      "required": true,
      "title": "Gemini API Key",
      "type": "password"
    },
    {
      "description": "Search API Key",
      "name": "searchApiKey",
      "required": false,
      "title": "Google Search API Key",
      "type": "password"
    },
    {
      "description": "Search Engine ID",
      "name": "searchEngineID",
      "required": false,
      "title": "Google Programmable Search Engine ID",
      "type": "textfield"
    },
    {
      "description": "Whether to use streamed IO or not",
      "name": "streamedIO",
      "label": "Whether to use streamed IO or not",
      "required": false,
      "title": "Streamed IO",
      "type": "checkbox"
    },
    {
      "description": "Whether to use mathjax-node to post-render math equations",
      "name": "enableMathjax",
      "label": "Whether to use mathjax-node to post-render math equations",
      "required": false,
      "title": "Enable mathjax",
      "type": "checkbox"
    },
    {
      "description": "Temperature used for text generation",
      "name": "temperature",
      "required": false,
      "title": "Temperature",
      "type": "textfield",
      "default": "0.01"
    },
    {
      "description": "Choose from the top tokens that cumulate to no more than probability P",
      "name": "topP",
      "required": false,
      "title": "Top P",
      "type": "textfield",
      "default": "0.05"
    },
    {
      "description": "Choose among top K tokens each time",
      "name": "topK",
      "required": false,
      "title": "Top K",
      "type": "textfield",
      "default": "3"
    }
  ],
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@raycast/api": "^1.56.0",
    "@raycast/utils": "^1.9.0",
    "cheerio": "^1.0.0-rc.12",
    "file-type": "^18.7.0",
    "html-to-text": "^9.0.5",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "eslint": "^7.32.0",
    "is-binary-path": "^2.1.0",
    "prettier": "^2.5.1",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "postinstall": "cd assets/markdownMath && npm install"
  }
}
